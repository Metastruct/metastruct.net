{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true,
        "node": true
    },
    "extends": [
		"@nuxtjs",
		"prettier",
		"prettier/vue",
		"plugin:prettier/recommended",
		"plugin:nuxt/recommended"
    ],
    "parserOptions": {
        "ecmaVersion": 12
    },
    "plugins": [
        "prettier"
    ],
    "rules": {
		"nuxt/no-cjs-in-config": "off",
		"no-console": "off",
		"max-len": ["off", 80, 4],
		"no-prototype-builtins": "off",
		"require-await": "off",
		"one-var": "off",
		"import/order": "off",
		"vue/require-prop-types": "off",
		"vue/component-tags-order": "off"
	}
}
